&lt;td bgcolor=&quot;#8ea8c4&quot; width=&quot;81%&quot; height=&quot;25&quot;&gt;&lt;strong&gt;Conférences et ateliers pouvant être programmés&lt;/strong&gt;&lt;/td&gt;
&lt;td bgcolor=&quot;#8ea8c4&quot; width=&quot;17%&quot;&gt;&lt;a id=&quot;haut3&quot; name=&quot;Cours&quot;&gt;&lt;/a&gt;&lt;a href=&quot;#haut&quot;&gt;HAUT&lt;/a&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;tr&gt;
&lt;td colspan=&quot;3&quot;&gt;&lt;img src=&quot;templates/bizblue/images/px.gif&quot; border=&quot;0&quot; width=&quot;1&quot; height=&quot;15&quot; /&gt;&lt;/td&gt;
