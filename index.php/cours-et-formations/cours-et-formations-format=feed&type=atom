&lt;td bgcolor=&quot;#8ea8c4&quot; width=&quot;76%&quot; height=&quot;25&quot;&gt;&lt;strong&gt;Magnétisme&lt;/strong&gt;&lt;/td&gt;
&lt;td bgcolor=&quot;#8ea8c4&quot; width=&quot;22%&quot;&gt;&lt;a href=&quot;#haut&quot;&gt;HAUT&lt;/a&gt;&lt;/td&gt;
