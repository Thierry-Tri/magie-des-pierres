&lt;td bgcolor=&quot;#8ea8c4&quot; width=&quot;2%&quot;&gt; &lt;/td&gt;
&lt;td bgcolor=&quot;#8ea8c4&quot; width=&quot;91%&quot; height=&quot;25&quot;&gt;&lt;strong&gt;&lt;span style=&quot;text-decoration: underline;&quot;&gt;Déroulement d'une séance de Thérapie Fondamentale&lt;/span&gt;&lt;/strong&gt; 
&lt;table style=&quot;width: 100%;&quot; border=&quot;0&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot;&gt;
&lt;tbody&gt;
&lt;tr&gt;
&lt;td valign=&quot;top&quot;&gt; &lt;/td&gt;
